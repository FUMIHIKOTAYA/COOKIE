<h1>新規登録</h1>
<%= form_with(model: @user, local: true) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <%= f.label :profile %>
  <%= f.text_area :profile %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit "新規登録" %>
<% end %>
